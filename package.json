{
	"name": "web-portfolio-website",
	"version": "0.0.1",
	"description": "Brand new portfolio site.",
	"keywords": [
		"joshua",
		"chagani",
		"portfolio"
	],
	"main": "index.js",
	"repository": "https://github.com/joshchagani/web-portfolio-website.git",
	"author": "Joshua Chagani",
	"license": "MIT",
	"scripts": {
		"clean": "rm build/*",
		"format": "prettier --write src/.",
		"start": "snowpack dev",
		"build": "snowpack build",
		"test": "jest \"src/**/*.test.tsx\"",
		"tsc": "tsc"
	},
	"publishConfig": {
		"access": "public"
	},
	"browserslist": ">0.2%, not dead, not op_mini all",
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"src/*": "prettier --write --ignore-unknown"
	},
	"devDependencies": {
		"@babel/core": "^7.12.10",
		"@babel/preset-env": "^7.12.11",
		"@babel/preset-react": "^7.12.10",
		"@snowpack/app-scripts-react": "^2.0.0",
		"@snowpack/plugin-dotenv": "^2.0.5",
		"@snowpack/plugin-react-refresh": "^2.4.0",
		"@snowpack/plugin-typescript": "^1.2.1",
		"@testing-library/react": "^11.2.3",
		"@types/chai": "^4.2.14",
		"@types/mocha": "^8.2.0",
		"@types/react": "^17.0.0",
		"@types/react-dom": "^17.0.0",
		"@types/snowpack-env": "^2.3.3",
		"@types/styled-components": "^5.1.7",
		"babel-jest": "^26.6.3",
		"chai": "^4.2.0",
		"husky": "^4.3.8",
		"jest": "^26.6.3",
		"lint-staged": "^10.5.3",
		"prettier": "^2.2.1",
		"snowpack": "^3.0.11",
		"typescript": "^4.1.3"
	},
	"dependencies": {
		"history": "^5.0.0",
		"react": "^17.0.1",
		"react-dom": "^17.0.1",
		"react-router-dom": "^6.0.0-beta.0",
		"styled-components": "^5.2.1"
	}
}
